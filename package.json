{
  "name": "easycdn",
  "version": "0.1.0",
  "description": "easycdn - aws cdk pattern to spawn AWS CloudFront",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint lib/*.ts",
    "test": "jest --no-cache --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "clean": "rm lib/*.js lib/*.d.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogfish/easycdn"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "*",
    "@aws-cdk/aws-route53": "*",
    "@aws-cdk/aws-route53-targets": "*",
    "@aws-cdk/aws-s3": "*",
    "@aws-cdk/core": "*",
    "@types/node": "15.6.0",
    "aws-cdk-pure": "^1.3.19"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
