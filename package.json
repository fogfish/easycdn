{
  "name": "easycdn",
  "version": "0.2.0",
  "description": "EasyCDN is AWS CDK L3 Construct to spawn AWS CloudFront.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "lint": "eslint lib/*.ts",
    "test": "jest --no-cache --coverage",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "prepare": "npm run build",
    "clean": "rm lib/*.js lib/*.d.ts test/*.js test/*.d.ts && rm -Rf dist"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogfish/easycdn"
  },
  "author": "fogfish",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "go": {
        "moduleName": "github.com/fogfish/go-cdk-easycdn",
        "packageName": ""
      }
    }
  },
  "dependencies": {
    "aws-cdk-lib": "^2.173.1",
    "constructs": "10.4.2"
  },
  "peerDependencies": {
    "constructs": "10.4.2"
  },
  "devDependencies": {
    "constructs": "10.4.2",
    "jsii": "^5.7.1",
    "jsii-pacmak": "^1.105.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "coveralls": "^3.1.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}