{
  "name": "easycdn",
  "version": "0.2.0",
  "description": "EasyCDN is AWS CDK L3 Construct to spawn AWS CloudFront.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "lint": "eslint lib/*.ts",
    "test": "jest --no-cache --coverage",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "prepare": "npm run build",
    "clean": "rm lib/*.js lib/*.d.ts test/*.js test/*.d.ts && rm -Rf dist"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogfish/easycdn"
  },
  "author": "fogfish",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "go": {
        "moduleName": "github.com/fogfish/go-cdk-easycdn",
        "packageName": ""
      }
    }
  },
  "dependencies": {
    "aws-cdk-lib": "^2.103.1",
    "constructs": "10.3.0"
  },
  "devDependencies": {
    "constructs": "10.3.0",
    "jsii": "^5.2.23",
    "jsii-pacmak": "^1.91.0",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}